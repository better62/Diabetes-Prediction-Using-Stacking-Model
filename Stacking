lier_idx\n","\n","# 함수 사용해서 이상치 값 삭제\n","outlier_idx = get_outlier(df=data_copy, column='Glucose', weight=1.5)\n","print(outlier_idx)\n","data_copy.drop(outlier_idx, axis=0, inplace=True)\n","\n","outlier_idx1 = get_outlier(df=data_copy, column='BMI', weight=1.5)\n","print(outlier_idx1)\n","data_copy.drop(outlier_idx1, axis=0, inplace=True)\n","\n","outlier_idx2 = get_outlier(df=data_copy, column='Age', weight=1.5)\n","print(outlier_idx2)\n","data_copy.drop(outlier_idx2, axis=0, inplace=True)\n","\n","outlier_idx3 = get_outlier(df=data_copy, column='Pregnancies', weight=1.5)\n","print(outlier_idx3)\n","data_copy.drop(outlier_idx3, axis=0, inplace=True)"],"execution_count":null,"outputs":[{"output_type":"stream","text":["Int64Index([20, 75, 182, 342, 349, 502, 812, 819, 972, 1340, 1595, 1602, 1896], dtype='int64')\n","Int64Index([  12,   49,   60,   81,  120,  125,  145,  177,  193,  247,  303,\n","             371,  426,  445,  494,  522,  673,  684,  706,  773,  841,  896,\n","             915,  964, 1130, 1141, 1163, 1230, 1278, 1283, 1303, 1335, 1351,\n","            1405, 1485, 1504, 1553, 1563, 1564, 1567, 1576, 1586, 1587, 1590,\n","            1624, 1679, 1761, 1772, 1794, 1870, 1881, 1902, 1941, 1946, 1966,\n","            1998],\n","           dtype='int64')\n","Int64Index([   7,  123,  363,  453,  459,  489,  537,  666,  674,  833,  923,\n","             929,  959,  994, 1123, 1131, 1281, 1512, 1518, 1548, 1571, 1616,\n","            1754, 1762, 1944],\n","           dtype='int64')\n","Int64Index([88, 159, 298, 455, 768, 925, 1225, 1317, 1456, 1514, 1909, 1980], dtype='int64')\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LCUhDk_Pvtnr","executionInfo":{"status":"ok","timestamp":1619839636210,"user_tz":-540,"elapsed":996,"user":{"displayName":"이나은","photoUrl":"","userId":"04674513291941738475"}},"outputId":"54929318-ead4-4b78-9111-01ae8380e32a"},"source":["outlier = [88, 159, 298, 455, 768, 925, 